. row section featured topspace
    h2 
        class section-title
        span Services
    $foreach page in site.Data.services
        div 
            class 
            . col-sm-6 col-md-3
                h3 ${ page.Title }
                    class text-center
                p ${ page.Summary }
                p 
                    class text-center
                    a Read more
                        href ${ page.Permalink }
                        class btn btn-action
